<h2 style="text-transform: capitalize;"><%= link_to product.name, product %> </h2>
<h4> Metrics </h4>
<ul>
	<% product.metrics.each do |f| %>
		<li>
			<%= f.value %>
		</li>
	<% end %>
</ul>

<h4> Goals </h4>
<ul>
	<% product.goals.each do |f| %>
		<li>
			<%= f.value %>
		</li>
	<% end %>
</ul>

<h4> Concepts </h4>
<ul>
	<% product.concepts.each do |f| %>
		<li>
			<%= link_to f.title, account_product_concept_path(f,product,@account) %>
			<br>
		</li>
	<% end %>
</ul>

